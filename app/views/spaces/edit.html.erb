<div class="container">
  <h1>Update your space</h1>

  <%= simple_form_for(@space) do |f| %>
      <%= f.input :title %>
      <%= f.input :address %>
      <%= f.input :photo, as: :file %>
      <%= f.input :photo_cache, as: :hidden %>
      <%= f.submit class: "btn btn-success"%>
  <% end %>

  <%= link_to 'Back', space_path, class: "btn btn-default"%>
</div>
